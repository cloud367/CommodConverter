<simulation>
  <control>
    <duration>10</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>

  <archetypes>
    <spec><path>commodconverter</path><lib>CommodconverterFacility</lib><name>CommodconverterFacility</name></spec>
    <spec><path>commodconverter</path><lib>CommodconverterRegion</lib><name>CommodconverterRegion</name></spec>
    <spec><path>commodconverter</path><lib>CommodconverterInst</lib><name>CommodconverterInst</name></spec>
  </archetypes>

  <facility>
    <name>OneFacility</name>
    <config>
      <CommodconverterFacility />
    </config>
  </facility>

  <region>
    <name>OneRegion</name>
    <config>
      <CommodconverterRegion />
    </config>
    <institution>
      <name>OneInst</name>
      <initialfacilitylist>
        <entry>
          <prototype>OneFacility</prototype>
          <number>1</number>
        </entry>
      </initialfacilitylist>
      <config>
        <CommodconverterInst />
      </config>
    </institution>
  </region>

</simulation>
